{
  "name": "aleo_starter_template",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf \"{node_modules,.next,out.cache}\"",
    "delete-local-modules": "rimraf ./node_modules/@demox-labs/aleo-wallet-adapter-react/node_modules ./node_modules/@demox-labs/aleo-wallet-adapter-reactui/node_modules ./.next",
    "configure-program": "tsc ./util/update-program.ts && node ./util/update-program.js",
    "build-program": "cd ./program && /home/mikenike/.cargo/bin/leo build",
    "copy-program": "tsc ./util/copy-program.ts && node ./util/copy-program.js",
    "program": "yarn configure-program && yarn build-program && yarn copy-program",
    "dev": "yarn delete-local-modules && next dev",
    "dev:ts": "yarn dev & yarn ts:watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@demox-labs/aleo-wallet-adapter-base": "0.0.23",
    "@demox-labs/aleo-wallet-adapter-leo": "0.0.25",
    "@demox-labs/aleo-wallet-adapter-react": "0.0.22",
    "@demox-labs/aleo-wallet-adapter-reactui": "0.0.36",
    "@tailwindcss/forms": "0.5.11",
    "@tailwindcss/typography": "0.5.19",
    "browserify-fs": "^1.0.0",
    "classnames": "2.5.1",
    "daisyui": "5.5.14",
    "json-rpc-2.0": "^1.6.0",
    "minimatch": "^10.1.1",
    "next": "15.2.4",
    "next-pwa": "5.6.0",
    "next-seo": "6.6.0",
    "next-themes": "0.4.6",
    "process": "^0.11.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-query": "3.39.3",
    "react-use": "17.6.0",
    "stream-browserify": "^3.0.0",
    "swiper": "12.0.3",
    "webpack": "^5.104.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "autoprefixer": "10.4.23",
    "dotenv": "16.4.5",
    "eslint": "8.17.0",
    "eslint-config-next": "15.2.4",
    "eslint-config-prettier": "9.1.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nesting": "^14.0.0",
    "prettier": "3.8.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  }
}
